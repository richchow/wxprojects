@import "/css/WeUI.wxss";

page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}

.weui-cell__hd image {
  margin-right: 5px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  border-radius: 8rpx;
}

.cell_ft_new {
  color: #14b3c7;
  font-size: 24rpx;
}

.weui-cell::before {
  border-top: none;
}

.weui-cells::after {
  border-bottom: none;
}

.weui-cell_active {
  background-color: #fff;
}

.weui-cells {
  background-color: #fafafa;
  font-size: 26rpx;
}

.weui-panel::after {
  border-bottom:none;

}
.weui-panel__bd image{
  width: 40px;
  height: 40px;
  border-radius: 8rpx;
}
.weui-media-box__title{font-size: 26rpx;}
.padding20{ padding: 20rpx 20rpx 20rpx 0 ;}
.floatleft{float:left;}
.floatright{float:right;}
.backWhite{background-color: #fff;}
.valign-middle{vertical-align: middle;}
.margin_top_20{ margin-top: 20rpx;}
.padding_top_20{ padding-top: 20rpx;}
.size28{font-size: 28rpx;}