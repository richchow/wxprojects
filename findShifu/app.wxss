@import "/css/WeUI.wxss";

page {
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  height: 100%;
}

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}

.weui-cell__hd image {
  margin-right: 5px;
  vertical-align: middle;
  width: 40px;
  height: 40px;
  border-radius: 8rpx;
}

.cell_ft_new {
  color: #14b3c7;
  font-size: 24rpx;
}

.weui-cell::before {
  border-top: none;
}

.weui-cells::after {
  border-bottom: none;
}

.weui-cell_active {
  background-color: #fff;
}

.weui-cells {
  background-color: #fafafa;
  font-size: 26rpx;
}

.weui-panel::after {
  border-bottom: none;
}

.weui-panel__bd image {
  width: 40px;
  height: 40px;
  border-radius: 8rpx;
}

.bg_panel_user {
  border-bottom: 4rpx #fafafa solid;
}

.bg_panel_userimg {
  width: 40px;
  height: 40px;
  border-radius: 50%;
}

.bg_panel_usercontent {
  font-size: 24rpx;
  padding-left: 20rpx;
}

.color_time {
  color: #bfbfbf;
}

.page__bd_spacing2 {
  padding-left: 100rpx;
}

.bg_panel_imglist {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  display: table;
  margin: 0;
}

.bg_panel_imglist_img {
  width: 200rpx;
  height: 200rpx;
  float: left;
  padding-right: 10rpx;
  padding-bottom: 10rpx;
}

.bg_panel_filelist {
  margin-top: 20rpx;
  background-color: #e6e6e6;
  height: 40rpx;
  line-height: 40rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin-right: 20rpx;
}

.bg_panel_filelist_file {
  width: 40rpx;
  height: 40rpx;
  float: left;
  padding-right: 20rpx;
  padding-left: 20rpx;
}

.bottom_qa_button {
  width: 50rpx;
  height: 50rpx;
  flex: 1;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  font-size: 26rpx;
  color: #8a8a8a;
  padding-left: 10rpx;
  line-height: 30rpx;
}

.weui-media-box__title {
  font-size: 26rpx;
}

.padding20 {
  padding: 20rpx 20rpx 20rpx 0;
}

.paddingleft20 {
  padding-left: 20rpx;
}

.floatleft {
  float: left;
}

.floatright {
  float: right;
}

.backWhite {
  background-color: #fff;
}

.valign-middle {
  vertical-align: middle;
}

.margin_top_20 {
  margin-top: 20rpx;
}

.padding_top_20 {
  padding-top: 20rpx;
}

.padding_bottom_20 {
  padding-bottom: 20rpx;
}

.margin_bottom_60 {
  margin-bottom: 100rpx;
}

.size28 {
  font-size: 28rpx;
}

.line {
  width: 100%;
  display: table;
}

.loading {
  width: 100%;
  height: 64px;
  display: flex;
  align-items: center;
  text-align: center;
}

.loadinggif {
  width: 32px;
  height: 32px;
  margin: 0 auto;
  line-height: 64px;
}

.unreadnum_view {
  width: 160rpx;
  opacity: 0.5;
  border: 1px solid #8a8a8a;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
  text-align: center;
  right: 0;
  display: flex;
  position: fixed;
  margin-top: 20rpx;
  background-color: #8a8a8a;
  z-index: 9999999999;
  flex-direction: row;
  align-items: center;
}

.unreadnum_view image {
  width: 40rpx;
  height: 40rpx;
  margin: auto 0;
}
.unreadnum_view .unread_red{
  position: absolute;
  width: 10rpx;
  height: 10rpx;
}
.unreadnum_view view{
  flex: 1;
}
.unreadnum_view .va_unreadnum {
  font-size: 24rpx;
  color: white;
  text-justify: inter-cluster;
}
.unreadnum_view2 {
  width: 60rpx;
  opacity: 0.5;
  border: 1px solid #8a8a8a;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
  text-align: center;
  right: 0;
  display: flex;
  position: fixed;
  margin-top: 20rpx;
  background-color: #8a8a8a;
  z-index: 9999999999;
  flex-direction: row;
  align-items: center;
}

.unreadnum_view2 image {
  width: 40rpx;
  height: 40rpx;
  margin: auto 0;
}
.unreadnum_view2 .unread_red{
  position: absolute;
  width: 10rpx;
  height: 10rpx;
}
.unreadnum_view2 view{
  flex: 1;
}

.margin_right_20 {
  margin-right: 20rpx;
}

.bottomBtn_more {
  position: fixed;
  line-height: 95rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  bottom: 15px;
  overflow: hidden;
  z-index: 500;
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx;
  right: 5px;
}

.bottomBtn_nomore {
  background-color: #fff;
  line-height: 95rpx;
  font-size: 30rpx;
  color: #fff;
  overflow: hidden;
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx;
}

.v_more {
  position: fixed;
  background-color: #fff;
  opacity: 0.8;
  border-radius: 20px;
  line-height: 95rpx;
  font-size: 30rpx;
  color: #fff;
  bottom: 20px;
  overflow: hidden;
  z-index: 500;
  width: 100rpx;
  height: auto;
  right: 10px;
  flex-direction: column;
  border: #cdcdcd 1px solid;
}

.bottomContactBtn {
  position: relative;
  background-color: #14b3c7;
  line-height: 95rpx;
  text-align: center;
  color: #fff;
  overflow: hidden;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin: 10rpx;
}

.bottomVIPBtn {
  background-color: #14b3c7;
  line-height: 95rpx;
  text-align: center;
  color: #fff;
  overflow: hidden;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin: 10rpx;
}

.ad_img {
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 80%;
  overflow: hidden;
  position: fixed;
  z-index: 2000;
  height: auto;
  margin: auto;
  border-radius: 5px;
}

.close-icon {
  left: 0;
  right: 0;
  top: 42%;
  bottom: 0;
  width: 40px;
  height: 40px;
  overflow: hidden;
  position: fixed;
  z-index: 3000;
  margin: auto;
}
