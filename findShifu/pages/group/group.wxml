<view class="page">
  <view class="page__bd">
    <view class="weui-cells weui-cells_after-title">
      <navigator url="/pages/chat/chat" class="weui-cell weui-cell_access" hover-class="weui-cell_active" wx:for="{{groupitems}}" wx:for-item="item">
        <view class="weui-cell__hd">
          <image src="{{item.img}}"></image>
        </view>
        <view class="weui-cell__bd">{{item.title}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access cell_ft_new">{{item.new}}</view>
      </navigator>
    </view>
  </view>
</view>
<view bindtap="bindToCreateChat" class="button_bottom_add">
  <image src="/images/add-w.svg" />
</view>