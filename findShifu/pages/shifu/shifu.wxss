.sf_card {
  margin: 20rpx;
  border: 10rpx #fff solid;
  background-color: #fafafa;
  height: 280px;
}

.sf_card_item {
  display: table;
  flex-direction: row;
  width: 100%;
}

.sf_card_item_col {
  display: table;
  flex-direction: column;
  padding-left: 20px;
  position: absolute;
}

.sf_card_item view {
  display: table;
  padding: 20rpx;
  line-height: 20px;
  width: 40%;
}

.sf_card .sf_card_userimg {
  width: 60px;
  height: 60px;
  float: right;
}

.sf_card_lab {
  width: 36rpx;
  height: 10px;
  float: light;
  line-height: 60rpx;
  padding-right: 40rpx;
}

.sf_card_item_more {
  float: right;
  font-size: 20rpx;
  line-height: 20rpx;
  height: 20rpx;
  text-align: right;
  top: 160rpx;
  position: relative;
}

.sf_card_item_more image {
  float: right;
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
}

.sf_item_lab {
  font-size: 24rpx;
  border-right: 4rpx #f8f8f8 solid;
  text-align: center;
  vertical-align: middle;
  padding: 10rpx;
  margin: 10rpx;
}

.sf_item_lab_right {
  font-size: 26rpx;
  text-align: center;
  vertical-align: middle;
  padding: 16rpx;
  margin: 10rpx;
}

.sf_item_img {
  width: 30rpx;
  height: 30rpx;
}

.bottom_button {
  position: fixed;
  background-color: #14b3c7;
  line-height: 80rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  bottom: 0px;
  overflow: hidden;
  z-index: 500;
  width: 100%;
  height: 80rpx;
  right: 0;
}

.sf_bottom_h {
  height: 50rpx;
}

.bg_panel_filelist {
  align-content: center;
  text-align: center;
  height: 40rpx;
  background-color: #e6e6e6;
  border: 1px #e6e6e6 solid;
  border-radius: 5px;
}

.labtime {
  text-align: left;
  float: left;
}

.mastername {
  font-size: 60rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
}

.sf_card_more {
  font-size: 24rpx;
}

.bottom_button_left {
  position: fixed;
  background-color: #14b3c7;
  height: 80rpx;
  line-height: 80rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  bottom: 10px;
  overflow: hidden;
  z-index: 500;
  width: 35%;
  border-top-right-radius: 18px;
  border-bottom-right-radius: 18px;
}

.bottom_button_right {
  right: 0;
  position: fixed;
  background-color: #14b3c7;
  height: 80rpx;
  line-height: 80rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  bottom: 10px;
  overflow: hidden;
  z-index: 500;
  width: 35%;
  border-top-left-radius: 18px;
  border-bottom-left-radius: 18px;
}
 .white {
  align-content: center;
  text-align: center;
  height: 40rpx;
  background-color: #fff;
  border: 1px #e6e6e6 solid;
  border-radius: 5px;
}
.sf_img_show{
  width:20px;
  height:10px;
  padding-top: 6rpx;
  padding-right: 5rpx;
}
.relative_230{
  top:230rpx;
  position: relative;
}
.labtip_checked {
  border: 1rpx solid #14b3c7;
  border-radius: 5px;
  padding: 6rpx 6rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 12px;
  background-color: #14b3c7;
  margin: 6rpx 6rpx;
  display: inline-block;
}
.sf_service_scroll{
  height: 100rpx;
  width: 100%;
  white-space: nowrap;
}
