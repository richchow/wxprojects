.userinfo{
  width:100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 60rpx;
  background-color: #fff;
}
.userinfo-avatar {
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx;
  border-radius: 50%;
  border:1px #e6e6e6 solid;
  
}
.weui-cell{
  margin: 20rpx 0;
  background-color: #fff;
  font-size: 16px;
}
.bottomBtn {
  position: fixed;
  background-color: #14b3c7;
  line-height: 95rpx;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  bottom: 0px;
  overflow: hidden;
  z-index: 500;
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx;
  border-radius: 50%;
  right: 0;
}