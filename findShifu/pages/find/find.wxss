.labtip {
  border: 1rpx solid #14b3c7;
  border-radius: 5px;
  padding: 10rpx 10rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 12px;
  background-color: #14b3c7;
  margin: 10rpx 10rpx;
  display: inline-block;
}

.weui-media-box__thumb {
  border-radius: 5px;
}

.weui-media-box__desc {
  -webkit-line-clamp: 4;
  line-height: 1.6;
}
