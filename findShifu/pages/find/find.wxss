.labtip {
  border: 1rpx solid #14b3c7;
  border-radius: 5px;
  padding: 10rpx 10rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 12px;
  background-color: #14b3c7;
  margin: 10rpx 10rpx;
  display: inline-block;
}

.weui-media-box__thumb {
  border-radius: 5px;
}

.weui-media-box__desc {
  -webkit-line-clamp: 2;
  line-height: 1.6;
}
.sf_item_lab_left {
  font-size: 24rpx;
  border-right: 4rpx #f8f8f8 solid;
  vertical-align: middle;
  padding-top: 10rpx;
}
.sf_item_lab {
  font-size: 24rpx;
  border-right: 4rpx #f8f8f8 solid;
  text-align: center;
  vertical-align: middle;
  padding-top: 10rpx;
}

.sf_item_lab_right {
  font-size: 26rpx;
  text-align: center;
  vertical-align: middle;
  padding-top: 10rpx;
}

.sf_item_img {
  width: 20rpx;
  height: 20rpx;
}
