.labtip {
    border: 1rpx solid #14b3c7;
    border-radius: 5px;
    padding: 10rpx 10rpx;
    box-sizing: border-box;
    color: #fff;
    font-size: 12px;
    background-color: #14b3c7;
    margin: 10rpx 10rpx;
    display: inline-block;
}

.weui-media-box__thumb {
    border-radius: 5px;
}

.weui-media-box__desc {
    -webkit-line-clamp: 2;
    line-height: 1.6;
}

.sf_item_lab_left {
    font-size: 24rpx;
    border-right: 4rpx #f8f8f8 solid;
    vertical-align: middle;
    padding-top: 10rpx;
    width: 25%;
    float: left;
}

.sf_item_lab {
    font-size: 24rpx;
    border-right: 4rpx #f8f8f8 solid;
    text-align: center;
    vertical-align: middle;
    padding-top: 10rpx;
    width: 30%;
    float: left;
    padding-left: 6rpx;
}

.sf_item_lab_right {
    font-size: 26rpx;
    text-align: center;
    vertical-align: middle;
    padding-top: 10rpx;
    float: left;
    padding-left: 6rpx;
}

.sf_item_img {
    width: 20rpx;
    height: 20rpx;
}
.sharebox {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 5000;
  background: #000;
  padding-top: 20rpx;
  height: 100%;
  opacity: 0.7;
  filter: alpha(opacity=70);
}
.v_m_modal {
  width: 80%;
  overflow: hidden;
  position: fixed;
  z-index: 5000;
  background: #fffffe;
  padding-top: 16px;
  display:table;
  text-align: center;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  border-radius: 5px;
  border: #cdcdcd 1px solid;
}

.v_m_modal image {
  width: 90%;
  height: auto;
  margin: 0 auto;
  border: #cdcdcd 1px solid;
}
.v_m_describe{
  font-size: 14px;
  color: #707070;
  width: 90%;
  margin: auto;
}
.v_m_bottom{
  border-top: #cdcdcd 1px solid;
  font-size: 20px;
  color: #14b3c7;
  font-weight: 500;
  margin-top: 10px;
  height: 50px;
  line-height: 50px;
}