.page {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  background-color: #f3f3f5;
}

.about_title {
  font-size: 14px;
  line-height: 20px;
  padding: 15px 15px 0 15px;
  text-indent: 1px;
  font-weight: 900;
}

.about_content {
  font-size: 13px;
  line-height: 20px;
  margin: 15px;
  padding-bottom: 15px;
  border-bottom: 1px solid #cdcdcd;
  text-indent: 27px;
}
.about_content2 {
  font-size: 13px;
  line-height: 20px;
  margin: 15px;
  padding-bottom: 15px;
  text-indent: 27px;
}
.about_content_title{
  font-weight: 700;
}
ul {
  list-style-position: inside;
  list-style-type:square;
}

li {
  display: block;
}
